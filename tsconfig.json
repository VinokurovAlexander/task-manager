{
    "compilerOptions": {
        "baseUrl": "src",
        "jsx": "react",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "moduleResolution": "Node",
        "strict": true,
        "module": "ESNext",
        "noUnusedParameters": true,
        "noUnusedLocals": true,
        "lib": [
            "dom",
            "WebWorker",
        ]
    },
    "include": [
        "src"
    ]
}
